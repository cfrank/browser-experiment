{
  "name": "browser-experiment",
  "private": true,
  "scripts": {
    "dev": "concurrently -n orch,ext -c blue,green \"pnpm --filter @browser-experiment/orchestrator dev\" \"pnpm --filter @browser-experiment/extension dev\"",
    "build": "pnpm --filter @browser-experiment/shared build && pnpm --filter @browser-experiment/orchestrator build && pnpm --filter @browser-experiment/extension build",
    "typecheck": "pnpm -r typecheck"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "typescript": "^5.7.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
